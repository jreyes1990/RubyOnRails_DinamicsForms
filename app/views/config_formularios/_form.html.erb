
<%= simple_form_for(@config_formulario) do |f| %>
  <%= f.error_notification %>
  <%= f.error_notification message: f.object.errors[:base].to_sentence if f.object.errors[:base].present? %>

  <div class="form-inputs">
    <%= f.input :empresa_id %>
    <%= f.input :area_id %>
    <%= f.association :tipo_formulario %>
    <%= f.input :nombre %>
    <%= f.input :descripcion %>
    <%= f.input :app_siga %>
    <%= f.input :user_created_id %>
    <%= f.input :user_created_id %>
    <%= f.input :usr_grab %>
    <%= f.input :usr_modi %>
    <%= f.input :estado %>
  </div>

  <div class="form-actions">
  
    <%= f.submit "Crear y continuar" %>
  
  </div>
<% end %>
